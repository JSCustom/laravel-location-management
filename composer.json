{
    "name": "jscustom/laravel-location-management",
    "description": "Laravel package for location management. Includes country, province, and more.",
    "type": "library",
    "require": {
        "php": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^6.0 || ^7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JSCustom\\LaravelLocationManagement\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jediael Saliba",
            "email": "jscustom@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "JSCustom\\LaravelLocationManagement\\Providers\\LaravelLocationManagementServiceProvider"
            ],
            "aliases": {
                "Qbo": "JSCustom\\LaravelLocationManagement\\Facade"
            }
        }
    }
}
